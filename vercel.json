{
  "version": 2,
  "builds": [
    {
      "src": "frontend/app-ui/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api-docs",
      "dest": "/api/index.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/app-ui/$1"
    }
  ]
}
